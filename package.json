  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "mcp:playwright": "node scripts/mcp/playwright-server.js",
    "mcp:supabase": "node scripts/mcp/supabase-server.js",
    "mcp:context7": "node scripts/mcp/context7-server.js",
    "e2e:login": "node scripts/e2e/login.js",
    "supabase:start": "npx -y supabase start",
    "supabase:status": "npx -y supabase status",
    "supabase:stop": "npx -y supabase stop",
    "supabase:db:push": "npx -y supabase db push",
    "supabase:functions:serve": "npx -y supabase functions serve --no-verify-jwt",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:contract": "jest --selectProjects contract-tests",
    "test:unit": "jest --selectProjects unit-tests",
    "test:integration": "jest --selectProjects integration-tests",
    "test:ci": "jest --coverage --watchAll=false --passWithNoTests"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@supabase/supabase-js": "^2.45.4",
