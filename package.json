    "jest": "^29.7.0",
    "jose": "^6.1.0",
    "playwright": "^1.48.2",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.14",
    "ts-jest": "^29.4.4",
    "typescript": "^5.6.3",
    "zod": "^3.23.8"
